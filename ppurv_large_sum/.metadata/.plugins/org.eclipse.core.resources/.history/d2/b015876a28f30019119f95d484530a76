#include "sum.h"

#include "inttypes.h"

int main(){

	int_least8_t sum1[] = {9, 8, 7, 6, 5};
	int_least8_t sum2[] = {9, 8, 7, 6, 5};
	//int_least8_t sum3[] = {0,0,0,0,0};
	int_least8_t sum3 = 0;

	int_least16_t sum[10]={};
	 int_least8_t len1 = sizeof(sum1) / sizeof (int_least8_t);
	int_least8_t i;
	int_least8_t sum4[10]={};
	for (i=len1-1;i>-1;i--){

		printf("%d ",sum1[i]);
		printf("%d ",sum2[i]);
		sum[i]=sum1[i]+sum2[i]+(sum3%10);
		sum3=sum[i]/10;
		if(i==0){
			sum4[i]=10+(sum[i]%10);
		}
		else{
			sum4[i]=sum[i]%10;
		}
		//printf("*****%*****"PRIdLEAST8, sum4);
		printf("     %d ",sum[i]);
		printf("     %d ",sum3);
		printf("\n");

	}

	for(i=0;i<=len1;i--){
		printf("%"PRIdLEAST8,sum4[i]);
	}



return 0;
}
