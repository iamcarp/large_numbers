#include "sum.h"

#include "inttypes.h"

int chrtoint(char symbol)
{
	int_least8_t i;
	for (i = 48; i<=57; i++)
	{
		if (toascii(i)==symbol)
			{
			return i-48;
			}
	}
	return 0;
}

//reverese string
char *stringReveresulte(char *str)
{
      char *begin, *end;

      if (! str || ! *str)
            return str;
      for (begin = str, end = str + strlen(str) - 1; end > begin; ++begin, --end)
      {
            *begin ^= *end;
            *end ^= *begin;
            *begin ^= *end;
      }
      return str;
}

void main(){
	char num1[80];
	char num2[80];

	printf("First Number:");
	scanf("%s", &num1);
	printf("\nSecond Number:");
	scanf("%s", &num2);

	sumNumbers(num1,num2);

}
